{
    "type": "section",
    "title": "Contact",
    "description": "Display a contact section with a form and a google map",
    "props": {
        "name": {
            "type": "Text",
            "title": "Name",
            "description": "Address name",
            "required": true
        },
        "street": {
            "type": "Text",
            "title": "Street",
            "description": "Address street with number",
            "required": true
        },
        "city": {
            "type": "Text",
            "title": "City",
            "description": "Address city with postcode",
            "required": true
        },
        "phone": {
            "type": "Text",
            "title": "Phone",
            "description": "Phone number"
        },
        "email": {
            "type": "Text",
            "title": "Email",
            "description": "Email address"
        },
        "facebookUrl": {
            "type": "Text",
            "title": "Facebook URL",
            "description": "Facebook URL"
        },
        "twitterUrl": {
            "type": "Text",
            "title": "Twitter URL",
            "description": "Twitter URL"
        },
        "instagramUrl": {
            "type": "Text",
            "title": "Instagram URL",
            "description": "Instagram URL"
        },
        "markers": {
            "type": "Object[]",
            "title": "Markers",
            "description": "Google map markers",
            "props": {
                "lat": {
                    "type": "Number",
                    "title": "Latitude",
                    "description": "Marker latitude",
                    "required": true
                },
                "lng": {
                    "type": "Number",
                    "title": "Longitude",
                    "description": "Marker longitude",
                    "required": true
                }
            }
        }
    }
}
